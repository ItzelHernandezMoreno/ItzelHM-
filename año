#include <iostream>
using namespace std;
int main(){
	string user="305";
	string password="que show";
	cout<<"Digita tu usuario"<<endl;
	cin>>user;
	cout<<"Digita tu contraseña"<<endl;
	cin>>password;
	if(user==user and password==password){
		cout<<"Bienvenido";
	}
	else{
		cout<<"Tu contraseña es incorrecta";
	}
}
